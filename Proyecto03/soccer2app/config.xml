<?xml version='1.0' encoding='utf-8'?>
<widget id="com.football2" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Football2</name>
    <description>Sample Apache Cordova App</description>
    <author email="dev@cordova.apache.org" href="https://cordova.apache.org">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <platform name="android">    
        <icon src="src/assets/logo/logo-color.png" density="mdpi" />
        <icon src="src/assets/logo/logo-color.png" density="ldpi" />
        <icon src="src/assets/logo/logo-color.png" density="hdpi" />
        <icon src="src/assets/logo/logo-color.png" density="xhdpi" />
        <icon src="src/assets/logo/logo-color.png" density="xxhdpi" />
    </platform>
</widget>
